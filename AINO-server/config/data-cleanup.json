{
  "cleanup_strategies": {
    "auto_cleanup": {
      "enabled": true,
      "retention_days": 30,
      "schedule": "0 2 * * *",
      "description": "自动删除30天前的软删除记录"
    },
    "test_data_cleanup": {
      "enabled": true,
      "keywords": ["test", "测试", "demo", "示例", "example", "临时"],
      "description": "自动清理包含测试关键词的记录"
    },
    "compression": {
      "enabled": false,
      "compress_after_days": 7,
      "description": "压缩7天后的软删除记录"
    },
    "archiving": {
      "enabled": false,
      "archive_after_days": 90,
      "description": "归档90天后的软删除记录"
    }
  },
  "tables": {
    "dir_users": {
      "retention_days": 30,
      "compress_fields": ["props"],
      "archive_enabled": true
    },

  },
  "monitoring": {
    "alert_thresholds": {
      "delete_rate": 0.8,
      "total_records": 10000,
      "deleted_records": 5000
    },
    "notifications": {
      "enabled": true,
      "email": "admin@aino.com",
      "webhook": null
    }
  }
}
